{
  "name": "address-book",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node build/src/app.js",
    "start:dev": "export DEBUG=dev && nodemon --exec babel-node server/src/app.js",
    "build": "npm run clean && babel -d ./build ./server --copy-files",
    "clean": "rm -rf build && mkdir build",
    "test": "export NODE_ENV=test && nyc --reporter=text mocha  --timeout 15000 --exit ./server/src/test/ --recursive -r @babel/register -r @babel/polyfill",
    "nyc": "nyc --reporter=lcov npm run test && nyc report --reporter=text-lcov | coveralls",
    "migrate": "sequelize db:migrate",
    "dev:migrate": "export DEBUG=dev && sequelize db:migrate",
    "undo:migration": "sequelize db:migrate:undo:all",
    "reset:db": "npm run undo:migration && npm run dev:migrate",
    "heroku:db": "npm run migrate",
    "heroku-postbuild": "npm run heroku:db && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "bcrypt": "^5.0.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "firebase": "^8.4.2",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.3.2",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.5.3",
    "firebase-server": "^1.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0"
  }
}
