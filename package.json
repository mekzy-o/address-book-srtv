    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.3.2",
    "morgan": "^1.10.0",
    "pg": "^8.6.0"
  },
  "devDependencies": {
