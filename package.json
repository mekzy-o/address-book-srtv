  "main": "index.js",
  "scripts": {
    "start": "DEBUG=http node server/dist/app ",
    "prestart": "npm run build",
    "start:dev": "export DEBUG=dev && nodemon --exec babel-node server/src/app.js",
    "build": "babel-node server/src/ -d server/dist/",
    "test": "NODE_ENV=test nyc --reporter=text mocha ./server/src/test/ --recursive -r @babel/register -r @babel/polyfill",
    "nyc": "nyc --reporter=lcov npm run test && nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "npm run build",
    "migrations": "export DEBUG=dev && babel-node server/src/database/dropTables.js && babel-node server/src/database/createTables.js"
  },
